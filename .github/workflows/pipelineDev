name: Pipeline DEV
 
on:
  push:
    branches: [ "Dev" ]
jobs:
  build:
    runs-on: ubuntu-latest
    steps:
    - uses: actions/checkout@v4
    - name: Set up JDK 11
      uses: actions/setup-java@v3
      with:
        java-version: '11'
        distribution: 'temurin'
    - name: Executando programa em DEV
      run: 
        javac src/Exercicio1.java -d out
    - name: Ambiente
      run: echo ESTAMOS EM DEV!!
